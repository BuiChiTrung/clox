
class Person {
  fun sayName() {
    print(this.name);
  }
}

var bill = Person();
bill.name = "Bill";
var method = bill.sayName;
method();

var manh = Person();
manh.name = "MAnh";
bill.sayName = manh.sayName;
bill.sayName(); 

fun do_sth(argument) {
    print("argument");
}

manh.x = do_sth;
manh.x("argument");