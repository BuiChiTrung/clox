var i = 0;

while i < 10 {
    i = i + 1;

    if i % 2 == 0 {
        continue;
    }

    print(i);

    if i == 7 {
        print("Reached", i, "breaking loop!");
        break;
    }
}

for var j = 0; j <= 12; j = j + 1; {
    if j % 2 == 0 {
        if j % 3 == 0 {
          continue;
        }
        if j % 3 == 1 {
          print(j);
        }
    }
    
    if j == 11 {
        print("Reached", j, "breaking loop!");
        break;
    }
}